{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "externalHelpers": true,
        "parser": {
            "syntax": "typescript",
            "tsx": true
        },
        "transform": {
            "react": {
                "runtime": "automatic",
                "development": false
            }
        },
        "target": "esnext",
        "experimental": {
            "plugins": [["@lingui/swc-plugin", {}]]
        }
    },
    "isModule": true
}
