import Page from "{{ dir.absolute }}/page.tsx";
{{#includes types "layout"}}
import Layout from "{{ dir.absolute }}/layout.tsx";
{{/includes}}
{{#includes types "template"}}
    import Template from "{{ dir.absolute }}/template.tsx";
{{/includes}}
import type { Meta, StoryObj } from "@storybook/react";

const meta = {
    component: Page,
    parameters: {},
    decorators: [
{{#includes types "layout"~}}
        (Story, ctx) => (
            <Layout {...ctx.args}>
                <Story/>
            </Layout>
        ),
{{/includes}}
{{#includes types "template"~}}
        (Story, ctx) => (
            <Template {...ctx.args}>
                <Story/>
            </Template>
        ),
{{/includes}}
    ],
} satisfies Meta<typeof Page>;

export default meta;

type Story = StoryObj<typeof Page>;

export const Primary: Story = {};
